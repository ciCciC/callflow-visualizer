  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "node test/clean.js",
    "lint": "eslint src --ext .ts",
    "full-test": "npm run lint && npm run test && npm run build",
    "test": "npm run clean && karma start test/karma.conf.js --single-run",
    "test-watch": "npm run clean && cross-env debug=true karma start test/karma.conf.js",
    "prerelease": "npm version prerelease --reid=SNAPSHOT",
    "release": "npm version",
    "webpack-run": "webpack --config test/webpack.dev.config.js --progress --watch --mode development",
    "serve-without-mocks": "npm run clean && concurrently 'webpack --config test/webpack.dev.config.js --progress --watch --mode development' 'node test/demo/server.js'",
    "serve-local-proxy": "npm run clean && concurrently 'webpack --config test/webpack.dev.config.js --progress --watch --mode development' 'node test/demo/server.js local-proxy'",
    "serve-local": "npm run clean && concurrently \"webpack --config test/webpack.dev.config.js --progress --watch --mode development\" \"node test/demo/server.js local\"",
    "start-server": "node test/demo/server.js",
    "start-static-server": "node test/demo/static-server.js",
    "start": "run-p clean webpack-run start-static-server"
  },
